<?xml version="1.0"?> 
<sdf version="1.4">
  <world name="trajectory_guided_world">

    <!-- Ground Plane -->
    <include>
      <uri>model://ground_plane</uri>
    </include>

    <include>
      <uri>model://sun</uri>
    </include>

    <!-- A low poly tram -->
    <include>
      <uri>model://tram</uri>
      <pose>0 0 0 0 0 0</pose>
      <plugin name="push_animate" filename="libtrajectory_guided.so"/>
    </include>
      
  </world>
</sdf>
